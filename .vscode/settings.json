{
    "editor.formatOnSave": true,
    "rust-analyzer.check.command": "clippy",
    "rust-analyzer.rustfmt.extraArgs": [
        "+nightly"
    ],
    "rust-analyzer.cargo.extraEnv": {
        "RUSTC_BOOTSTRAP": "1",
    },
    "rust-analyzer.cargo.targetDir": true,
    "cSpell.words": [
        "Addrs",
        "appender",
        "chrono",
        "httpmock",
        "logfile",
        "rfind",
        "rsplit",
        "rust-proxy",
        "serde",
        "thiserror",
        "userinfo"
    ]
}